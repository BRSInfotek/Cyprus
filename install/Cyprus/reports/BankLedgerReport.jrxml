	<queryString>
		<![CDATA[SELECT  
AD_PInstance_ID,
       ad_org_id            ,
  ad_client_id         ,
  c_bpartner_id        ,
   C_ACCTSCHEMA_ID,
  ACCT_SCHEMA,
  ACCOUNTNO,
  BANK_NAME,
  C_BankAccount_ID,
  C_BANK_ID,
  SO_CREDITLIMIT ,
 DOCUMENTNO ,
  BPARTNER_CODE        ,
  DATEACCT             ,
  ISPDC ,
  Logo_Text,
  OrgInfo_Tax_ID,
  DOCTYPE_NAME,
   PAYMENT_TERM,
  BPARTNER_NAME        ,
BP_ADDRESS,
BP_PHONE,
BP_FAX,
  org_name             ,
  DESCRIPTION ,
    PREVIOUS_BAL         ,
  DEBIT                ,
  CREDIT               ,
  PDCAMT               ,
  dateacct_start       ,
  dateacct_end  ,ISO_CODE  ,ORG_TRN ,CHECKNO ,RECONCILED,
   case when Previous_Bal < 0 then abs(Previous_Bal) else 0 end as Prev_Credit, 
   case when Previous_Bal > 0 then Previous_Bal else 0 end as Prev_Debit, 
 previous_bal + sum(debit - credit) over (order by rownum) as Running_Balance 
   FROM RV_BANK_LEDGER
   where DATEACCT >= To_Date($P{DateAcct1}) and
    DATEACCT <=  To_Date($P{DateAcct2}) 
   ]]>
	</queryString>
