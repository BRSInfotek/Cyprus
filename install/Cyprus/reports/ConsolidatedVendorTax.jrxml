	<queryString>
		<![CDATA[select  distinct ORG_NAME,ISO_CODE,CUSTOMER,TRN,TRN_TEXT,LOGO_TEXT,
BP_GROUP_NAME,TAXNAME,RATE,
BP_CODE,Acct_schema,
sum(taxamount) over(partition by bp_code,rate)taxamount_fc,
sum(totaltaxableamt) over(partition by bp_code,rate)totaltaxableamt_fc,

sum(taxamt_base) over(partition by bp_code,rate)taxamt_base,
sum(totaltaxableamt_base) over(partition by bp_code,rate)totaltaxableamt_base
from V_APSALESTAX_CONS_STD
where
 (AD_Client_ID= $P{AD_Client_ID}  ) AND
 (C_AcctSchema_ID= $P{C_AcctSchema_ID}  ) AND
 (AD_Org_ID= $P{AD_Org_ID}   ) AND
(C_BPartner_ID= $P{C_BPartner_ID}  OR  $P{C_BPartner_ID}=9999999) AND
 ( C_BP_Group_ID=$P{C_BP_Group_ID} OR  $P{C_BP_Group_ID}=9999999) AND
 (C_Tax_ID= $P{C_Tax_ID}    OR   $P{C_Tax_ID} =9999999) AND
  (dateinvoiced >= To_Date( $P{DateFrom}   )  OR      $P{DateFrom} =To_Date('01/01/2000','dd/mm/yyyy'))
AND 
(dateinvoiced <= To_Date(  $P{DateTo}  ) OR     $P{DateTo} = GETDATE())
order by BP_CODE,rate]]>
	</queryString>
