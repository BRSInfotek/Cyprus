	<queryString>
		<![CDATA[SELECT AD_PInstance_ID,      AD_CLIENT_ID		,C_ELEMENTVALUE_ID	,nvl(C_ACTIVITY_ID,0) as	C_ACTIVITY_ID,
DOCUMENTNO,DOCTYPE,LEDGERCODE	, LEDGERNAME	,GL_JOURNAL_ID	,GL_JOURNALBATCH_ID	,
FACT_ACCT_CREATED,FACT_ACCT_ID,DEBIT	,CREDIT	,PREV_BAL ,DESCRIPTION	,JOURNALNO	,
JOURNALDATE	,BATCHNO	,ORG_NAME	,DATEACCT	,TENANT_NAME	,TENANT_DESC	,POSTINGTYPE	,ACTIVITY	,
C_ACCTSCHEMA_ID	,ACCT_SCHEMA	,DATEACCT_START ,DATEACCT_END ,V_ORG_NAME,ISO_CODE,
CREATED,Logo_Text,nvl(ad_org_id,0) ad_org_id,nvl(v_ad_org_id,0) v_ad_org_id,
(LEDGERCODE||' '||LEDGERNAME) as glname,
 case when PREV_BAL < 0 then abs(PREV_BAL) else 0 end as Prev_Credit, 
   case when PREV_BAL > 0 then PREV_BAL else 0 end as Prev_Debit,
--sum(debit+(debit-credit)) as balance,
nvl(PREV_BAL,0) + (sum(debit-credit) OVER (partition by LEDGERCODE ORDER BY ROWNUM)) as balance

FROM RV_TEMP_STATEMENT_ACCT]]>
