	<queryString>
		<![CDATA[select   invoice_date,tax_amount,taxamt_base,
invoice_no,
Logo_Text,sales_rep,
Activity,
TOTALLINEAMOUNT,
sum(TOTALLINEAMOUNT) over (partition by invoice_no) as totallineamtnew,
TOTALLINEAMOUNT_FC,
FN_GET_PREV_BAL_FOR_INVOICE('CUSTOMER', To_Date($P{DateFrom}),To_Date( $P{DateTo} ) ) as Prev_Bal,
BPartner_Grp_Name as BPGroupName,
order_no,customer as vendor,doc_type,prod_name,acct_schema,prod_cat_name,organization_name,
uom,currency
,qtyinvoiced,price_base,
invoice_no,
product as productcode,price,line_amount,tax_rate,line_amount_fc,grandtotal,price_base,tax_amount,
 grandtotal_bc ,
(select sum(sumgrand)  from 
	(select distinct sum(TOTALLINEAMOUNT) over (partition by invoice_no) as sumgrand, invoice_no from v_customer_invoice_detail  
		where
		 AD_Client_ID = $P{AD_Client_ID} AND
  C_AcctSchema_ID = $P{C_AcctSchema_ID} AND
 (AD_Org_ID =  $P{AD_Org_ID} OR $P{AD_Org_ID} = 9999999) AND
  (C_Activity_ID =  $P{C_Activity_ID} OR $P{C_Activity_ID} = 9999999) AND
invoice_date >=To_Date($P{DateFrom})  and
invoice_date <= To_Date( $P{DateTo} ) AND
(C_BP_Group_ID =  $P{C_BP_Group_ID} OR $P{C_BP_Group_ID} = 9999999) AND
 (C_BPartner_ID = $P{C_BPartner_ID} OR $P{C_BPartner_ID} = 9999999) AND
 (M_Product_Category_ID = $P{M_Product_Category_ID} OR $P{M_Product_Category_ID} = 9999999) AND
  (M_Product_ID = $P{M_Product_ID} OR $P{M_Product_ID} = 9999999 ) AND
 ( DOCUMENT_STATUS = $P{DocStatus} OR $P{DocStatus} = 'ALL' ) ) ) as sumgrandtotal
  from  v_customer_invoice_detail 
where 
 AD_Client_ID = $P{AD_Client_ID} AND
  C_AcctSchema_ID = $P{C_AcctSchema_ID} AND
 (AD_Org_ID =  $P{AD_Org_ID} OR $P{AD_Org_ID} = 9999999) AND
  (C_Activity_ID =  $P{C_Activity_ID} OR $P{C_Activity_ID} = 9999999) AND
invoice_date >=To_Date($P{DateFrom})  and
invoice_date <= To_Date( $P{DateTo} ) AND
(C_BP_Group_ID =  $P{C_BP_Group_ID} OR $P{C_BP_Group_ID} = 9999999) AND
 (C_BPartner_ID = $P{C_BPartner_ID} OR $P{C_BPartner_ID} = 9999999) AND
 (M_Product_Category_ID = $P{M_Product_Category_ID} OR $P{M_Product_Category_ID} = 9999999) AND
  (M_Product_ID = $P{M_Product_ID} OR $P{M_Product_ID} = 9999999 ) AND
 ( DOCUMENT_STATUS = $P{DocStatus} OR $P{DocStatus} = 'ALL' ) 

 order by invoice_date]]>
