		<![CDATA[SELECT  ad_org_id           , 
 ad_client_id        , 
  c_bpartner_id       ,  
  c_bp_GROUP_id       ,  
  DATEINVOICED,  
  C_AgingBucket_ID  ,
  BUCKET_NAME,  VENDOR, 
   BPGROUP_NAME      ,  
   org_name            , 
    org_addr            , 
     org_phone           ,  
     DUEAMT               ,  
     CURRENT_DUEAMT       ,  
     PREVIOUS_DUE_AMT     , 
      OPENAMT              ,  
      PAIDAMT              , 
      nvl((openamt-paidamt),0) as amount,
       BUCKET_DETAILS       , 
        tenant_name         ,  
        tenant_desc         , 
         DATEacct_start      , 
          DATEacct_end        ,  
            ISO_CODE, 
             C_CURRENCY_ID, 
             AD_PInstance_ID,
              C_ACCTSCHEMA_ID,
               ACCT_NAME ,
                C_COUNTRY_ID,
                  COUNTRY,
                  LOGO_TEXT,
                  ISPDC
                   FROM TEMP_EMP_AGING_INV
                   WHERE
                   AD_CLIENT_ID =  $P{AD_Client_ID}  AND
