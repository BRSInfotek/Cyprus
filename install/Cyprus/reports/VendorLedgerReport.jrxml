	<queryString>
		<![CDATA[select  TO_CHAR(TO_DATE(DATEACCT_START, 'dd-mm-rr'), 'dd/mm/yyyy') as DATEACCT_START,
TO_DATE(DATEACCT, 'dd-mm-rr') as DATEACCT,
DOCTYPE_NAME,TAXID,CUST_ADDRESS,CUST_PHONE,
BPARTNER_CODE,
DOCUMENTNO,
customer_name,
vendor_name,
DESCRIPTION,
ISO_CODE,
DEBIT_FC,org_addr,
CREDIT_FC,
DEBIT,
CREDIT,
Logo_Text,
previous_bal,debit-credit as net,
   case when previous_bal < 0 then abs(Previous_Bal) else 0 end as Prev_Credit, 
   case when previous_bal > 0 then Previous_Bal else 0 end as Prev_Debit,
--sum(debit+(debit-credit)) as balance,
nvl(previous_bal,0) + (sum(debit-credit) OVER(PARTITION BY BPARTNER_CODE ORDER BY ROWNUM)) as balance,
TO_CHAR(TO_DATE(DUEDATE, 'dd-mm-rr'), 'dd/mm/yyyy') as DUEDATE,
 TO_CHAR(TO_DATE(PAID_DATE, 'dd-mm-rr'), 'dd/mm/yyyy') as PAID_DATE,cust_address,
ORG_PHONE,
ORG_FAX,org_name,
BPARTNER_CODE,SALES_PERSON,acct_schema,
SO_CREDITLIMIT,
PAYMENT_TERM,
FN_GET_TOTAL_DEBIT('VENDOR',To_Date( $P{DateAcct1}) ,To_Date( $P{DateAcct2} ), C_CURRENCY_ID) as OverAllDebit,
FN_GET_TOTAL_CREDIT('VENDOR',To_Date( $P{DateAcct1}) ,To_Date( $P{DateAcct2} ), C_CURRENCY_ID) as OverAllCredit
 from   RV_VENDOR_LEDGER
 where
 (DATEACCT  >= To_Date( $P{DateAcct1} ) AND DATEACCT <=To_Date( $P{DateAcct2} ))]]>
	</queryString>
