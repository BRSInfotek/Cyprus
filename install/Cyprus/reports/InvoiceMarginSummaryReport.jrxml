	<queryString language="SQL">
		<![CDATA[select distinct BB.ORGANIZATION_NAME,BB.ACCT_NAME, BB.invoice_no, BB.invoice_date,BB.customer,BB.SALES_REP,Activity,
BB.TOTALLINES_base,(select  sum(QTY_INVOICED*currentcostprice5) from V_CUSTOMER_INV_MARGIN_SUMMARY  where INVOICE_NO=BB.INVOICE_NO) as totalcostnew,
nvl(TOTALLINES_base-((select  sum(QTY_INVOICED*currentcostprice5) from V_CUSTOMER_INV_MARGIN_SUMMARY  where INVOICE_NO=BB.INVOICE_NO)),0 ) as MARGIN_NEW,

nvl((((TOTALLINES_base-((select   sum(QTY_INVOICED*COST) from V_CUSTOMER_INV_MARGIN_SUMMARY  where INVOICE_NO=BB.INVOICE_NO)) ) 
 * 100)/NULLIF(BB.TOTALLINES_base,0)),0) as marginpercentagenew,BB.C_Invoice_ID,BB.AD_Client_ID,BB.C_AcctSchema_ID,BB.AD_Org_ID,BB.C_BPartner_ID
 ,BB.C_Invoice_ID
  from  V_CUSTOMER_INV_MARGIN_SUMMARY BB 
 where 
 AD_Client_ID= $P{AD_Client_ID} and
C_AcctSchema_ID= $P{C_AcctSchema_ID} and
(AD_Org_ID= $P{AD_Org_ID} or  $P{AD_Org_ID}=9999999) and
(C_BPartner_ID= $P{C_BPartner_ID} or  $P{C_BPartner_ID}=9999999) and
(C_Invoice_ID= $P{C_Invoice_ID} or  $P{C_Invoice_ID}=9999999) and
(C_Activity_ID= $P{C_Activity_ID}   or   $P{C_Activity_ID} =9999999) and
INVOICE_DATE  >=  $P{DateFrom}  and
INVOICE_DATE <=  $P{DateTo}
order by INVOICE_DATE asc]]>
	</queryString>
