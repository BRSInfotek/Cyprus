             xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/application_5.xsd"
             version="5">
	<display-name>Cyprus Applications</display-name>
	<module>
		<ejb>cyprusRoot.jar</ejb> 
	</module>
	<module>
		<web>
			<web-uri>cyprusRoot.war</web-uri>
			<context-root>/admin</context-root>
		</web>
	</module>
	<module>
		<web>
			<web-uri>cyprusApps.war</web-uri>
			<context-root>/cyprus</context-root>
		</web>
	</module>
 	<module>
		<web>
			<web-uri>cyprusWebStore.war</web-uri>
			<context-root>/wstore</context-root>
		</web>
	</module>
	<module>
		<web>
			<web-uri>cyprusWebCM.war</web-uri>
			<context-root>/</context-root>
		</web>
	</module>
<!-- 	<module>
		<web>
			<web-uri>posterita.war</web-uri>
			<context-root>/posterita</context-root>
		</web>
	</module>  -->
	<module>
		<web>
			<web-uri>webui.war</web-uri>
			<context-root>/webui</context-root>
		</web>
